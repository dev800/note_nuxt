{
  "name": "note",
  "version": "1.0.0",
  "description": "Note with nuxt&#39;s project",
  "author": "happy",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.ts --watch server",
    "build": "nuxt-ts build --config-file nuxt.config.ts",
    "start": "cross-env NODE_ENV=production ts-node server/index.ts",
    "generate": "nuxt-ts generate --config-file nuxt.config.ts",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "fix": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/babel-preset-app": "^2.10.1",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^3.0.0-0",
    "cross-env": "^6.0.3",
    "element-ui": "^2.4.11",
    "koa": "^2.6.2",
    "nuxt": "^2.10.1",
    "ts-node": "^8.4.1",
    "vue-class-component": "^7.1.0",
    "vue-meta": "^2.3.1",
    "vue-property-decorator": "^8.3.0",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@nuxt/typescript": "^2.8.1",
    "@nuxt/typescript-build": "^0.3.1",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^3.0.9",
    "jest": "^24.1.0",
    "lint-staged": "^9.4.2",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "vue-jest": "^4.0.0-0"
  }
}
